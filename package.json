{
  "name": "teanblog",
  "version": "1.0.0",
  "description": "A minimalist style blog based on Egg.js & Vue.js",
  "private": true,
  "dependencies": {
    "axios": "^0.17.1",
    "egg": "^2.3.0",
    "egg-scripts": "^2.5.1",
    "egg-view-nunjucks": "^2.1.4",
    "vue": "^2.5.13",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "autoprefixer": "^8.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "cz-conventional-changelog": "^2.1.0",
    "egg-bin": "^4.3.7",
    "egg-ci": "^1.8.0",
    "egg-mock": "^3.14.0",
    "eslint": "^4.17.0",
    "eslint-config-egg": "^6.0.0",
    "parcel-bundler": "^1.6.1",
    "parcel-plugin-vue": "^1.5.0",
    "postcss-modules": "^1.1.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "vue-template-compiler": "^2.5.13",
    "webstorm-disable-index": "^1.2.0"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "scripts": {
    "start": "egg-scripts start --daemon --port=8003 --title=egg-server-teanblog",
    "build": "parcel build app/web/index.html --no-cache --public-url ./",
    "stop": "egg-scripts stop --title=egg-server-teanblog",
    "dev:index": "egg-bin dev",
    "dev:admin": "parcel app/web/index.html",
    "debug": "egg-bin debug",
    "fix": "npm run lint -- --fix",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov",
    "autod": "autod"
  },
  "ci": {
    "version": "8"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "TeanLee",
  "license": "MIT",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
