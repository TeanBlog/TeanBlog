{
  "name": "teanblog",
  "version": "1.0.0",
  "description": "A minimalist style blog based on Egg.js & Vue.js",
  "private": true,
  "dependencies": {
    "bcrypt": "^1.0.3",
    "egg": "^2.3.0",
    "egg-scripts": "^2.5.1",
    "egg-sequelize": "^3.1.1",
    "egg-view-nunjucks": "^2.1.4",
    "jsonwebtoken": "^8.1.1",
    "koa-jwt": "^3.3.0",
    "ms": "^2.1.1",
    "mysql2": "^1.5.2",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "cz-conventional-changelog": "^2.1.0",
    "egg-bin": "^4.3.7",
    "egg-ci": "^1.8.0",
    "egg-mock": "^3.14.0",
    "eslint": "^4.17.0",
    "eslint-config-egg": "^6.0.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.1",
    "gulp-less": "^3.5.0",
    "gulp-minify-css": "^1.2.4",
    "gulp-rev": "^8.1.1",
    "gulp-rev-collector": "^1.2.4",
    "gulp-uglify": "^3.0.0",
    "webstorm-disable-index": "^1.2.0"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "scripts": {
    "migrate:new": "egg-sequelize migration:create --name create-admin",
    "migrate:up": "egg-sequelize db:migrate --name",
    "migrate:down": "egg-sequelize db:migrate:undo --name",
    "start": "egg-scripts start --daemon --port=8003 --title=egg-server-teanblog",
    "stop": "egg-scripts stop --title=egg-server-teanblog",
    "dev": "egg-bin dev --port=8003",
    "debug": "egg-bin debug",
    "fix": "npm run lint -- --fix",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov",
    "autod": "autod"
  },
  "ci": {
    "version": "8"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "TeanLee",
  "license": "MIT",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
